<view class="traceabilityBar" style="{{'height:'+(bottomHegiht)+';'}}"><view class="tarbar-list-ul"><view data-event-opts="{{[['tap',[['setSelected',[index]]]]]}}" class="tarbar-list-li" onTap="__e" a:for="{{tabBar}}" a:for-item="item" a:for-index="index" a:key="index"><view class="tarbar-list-li-icon"><image src="{{item.iconPath}}" mode></image></view><view class="tarbar-list-li-name">{{item.text}}</view></view><view data-event-opts="{{[['tap',[['Buy',['$event']]]]]}}" class="buy" onTap="__e"><image src="../static/iconfont/buycart.png"></image><view class="text">再次购买</view></view></view></view>