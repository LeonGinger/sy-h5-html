.connect{width:90%;height:140rpx;margin:auto;display:-webkit-box;display:-webkit-flex;display:flex;position:absolute;margin-top:-40rpx;z-index:100;left:40rpx}.connect-left{margin-left:50rpx;position:absolute}.connect-right{right:0;position:absolute;margin-right:100rpx}.circular-up{width:40rpx;height:40rpx;background-color:#ededeb;-webkit-border-radius:50%;border-radius:50%;-webkit-box-shadow:-5rpx 5rpx 8rpx #dcdfe6 inset;box-shadow:-5rpx 5rpx 8rpx #dcdfe6 inset;position:absolute}.circular-center{width:15rpx;height:75rpx;background-color:#fff;-webkit-border-radius:20rpx;border-radius:20rpx;-webkit-box-shadow:0 0 3rpx #dcdfe6 inset;box-shadow:0 0 3rpx #dcdfe6 inset;position:absolute;margin-top:20rpx;margin-left:13rpx}.circular-down{width:40rpx;height:40rpx;background-color:#ededeb;-webkit-border-radius:50%;border-radius:50%;-webkit-box-shadow:-5rpx 5rpx 6rpx #dcdfe6 inset;box-shadow:-5rpx 5rpx 6rpx #dcdfe6 inset;position:absolute;margin-top:70rpx}